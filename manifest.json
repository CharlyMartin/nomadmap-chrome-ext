{
"manifest_version": 2,

"name": "",
"description": "Send an daily request to french nomad map, to update your location",
"version": "1.0.0",
"permissions": [
"storage",
"alarms",
"background",
"geolocation",
"http://*/",
"https://*/"
],
"background": {
"scripts": ["background.js"]
},
"options_ui": {
"page": "options.html",
"chrome_style": true
},
"icons": {
"160": "icon.png"
}
}
